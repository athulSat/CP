#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t,a,b,c,d,count(0);
    cin>>t;
    while(t--)
    {
        int arr[4];
        for(int i=0;i<4;i++)
        {
            cin>>arr[i];
        }
        for(int i=1;i<4;i++)
        {
            if(arr[i]>arr[0])count++;
            else continue;
        }
        cout<<count<<endl;
        count=0;
        
    }
    return 0;
}
